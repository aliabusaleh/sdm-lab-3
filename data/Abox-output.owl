  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://www.sdm-lab.com/#WritesReview">
    <rdfs:domain rdf:resource="http://www.sdm-lab.com/#Reviewer"/>
    <rdfs:range rdf:resource="http://www.sdm-lab.com/#Review"/>
