    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="http://www.sdm-lab.com/#Paper">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="http://www.sdm-lab.com/#ReviewsPaper"/>
        </owl:onProperty>
        <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
